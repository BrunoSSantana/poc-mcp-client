# Chave de API da Anthropic (necessária para o modelo Claude)
CLAUDE_API_KEY=sua_chave_api_anthropic

# Modelo Claude (opcional, padrão: claude-3-5-sonnet-20241022)
CLAUDE_MODEL=claude-3-5-sonnet-20241022

# Chave de API da OpenAI (necessária para o modelo GPT)
OPENAI_API_KEY=sua_chave_api_openai

# Modelo OpenAI (opcional, padrão: gpt-4o)
OPENAI_MODEL=gpt-4o

# Configuração do MCP
MCP_API_KEY=sua_chave_api_mcp
MCP_GRAPHQL_API=sua_url_graphql_mcp
MCP_MODEL=mcp-default
